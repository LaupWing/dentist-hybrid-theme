{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "dentist-hybrid/doctors-section",
  "version": "1.0.0",
  "title": "Doctors Section",
  "category": "theme",
  "icon": "admin-users",
  "description": "Display specialist doctors in a grid layout",
  "keywords": ["doctors", "team", "specialists"],
  "textdomain": "dentist-hybrid-theme",
  "attributes": {
    "sectionLabel": {
      "type": "string",
      "default": "Specialist Tandarts"
    },
    "heading": {
      "type": "string",
      "default": "Zorgzame Handen,<br />Professionele Tandheelkundige<br />Expertise"
    },
    "description": {
      "type": "string",
      "default": "Ons medelevende team combineert zorgzame handen met professionele tandheelkundige expertise om zachte, effectieve behandelingen te bieden voor jouw gezondste glimlach."
    },
    "buttonText": {
      "type": "string",
      "default": "Boek Nu"
    },
    "buttonLink": {
      "type": "string",
      "default": "/contact"
    }
  },
  "supports": {
    "align": false,
    "html": false
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}
